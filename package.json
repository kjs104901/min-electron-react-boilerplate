{
  "name": "min-electron-react-boilerplate",
  "version": "1.0.0",
  "description": "minimal electron react boilerplate",
  "main": "main.js",
  "scripts": {
    "bundle": "webpack --mode development",
    "serve": "electron .",
    "start": "npm-run-all --parallel bundle serve",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kjs104901/min-electron-react-boilerplate.git"
  },
  "keywords": [
    "electron",
    "react",
    "boilerplate"
  ],
  "author": "kjs104901",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "electron": "^5.0.0",
    "electron-builder": "^20.39.0",
    "electron-reload": "^1.4.0",
    "file-loader": "^3.0.1",
    "mini-css-extract-plugin": "^0.6.0",
    "npm-run-all": "^4.1.5",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "build": {
    "appId": "com.kjs104901.electronReact",
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "./asset/icon.ico"
    },
    "nsis": {
      "oneClick": true,
      "createDesktopShortcut": "always",
      "installerHeaderIcon": "./asset/icon.ico"
    }
  }
}